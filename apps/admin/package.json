    "@vueuse/shared": "^10.7.2",
    "@zxcvbn-ts/core": "^3.0.4",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.7",
    "codemirror": "^5.65.16",
    "cropperjs": "^1.6.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "exceljs": "^4.4.0",
    "ids": "1.0.5",
    "intro.js": "^7.2.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "resize-observer-polyfill": "^1.5.1",
    "showdown": "^2.1.0",
    "socket.io-client": "^4.7.4",
    "sortablejs": "^1.15.2",
