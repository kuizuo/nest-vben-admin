{
  "name": "backend",
  "version": "0.1.0",
  "description": "基于NestJs + TypeScript + TypeORM + Redis + MySql + Vben Admin编写的一款前后端分离的权限管理系统",
  "packageManager": "pnpm@7.11.0",
  "author": {
    "name": "kuizuo",
    "email": "hi@kuizuo.cn",
    "url": "https://github.com/kuizuo"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:test": "pm2 start ecosystem.config.js --env test",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "hygen module new"
  },
  "dependencies": {
    "@fastify/multipart": "^7.3.0",
    "@fastify/static": "^6.5.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.6",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.6",
    "@types/ioredis": "^5.0.0",
    "@types/lodash": "^4.14.186",
    "axios": "^1.1.3",
    "bull": "^4.10.1",
    "cache-manager": "^5.1.1",
    "chalk": "^5.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cron-parser": "^4.6.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.6",
    "dotenv": "16.0.3",
    "fastify": "^4.9.2",
    "fastify-multer": "^2.0.3",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "nanoid": "^4.0.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.3",
    "stacktrace-js": "^2.0.2",
    "svg-captcha": "^1.4.0",
    "systeminformation": "^5.12.11",
    "typeorm": "^0.3.10",
    "ua-parser-js": "^1.0.32",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "29.2.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.3",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
